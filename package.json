{
  "name": "vue-material-design-icons",
  "version": "2.4.0",
  "author": "Rob Cresswell <robcresswell@protonmail.com>",
  "license": "MIT",
  "description": "A collection of material design icons as Vue single file components",
  "repository": "gitlab:robcresswell/vue-material-design-icons",
  "keywords": [
    "vue",
    "material design icons",
    "mdi",
    "icons",
    "material",
    "icon",
    "svg",
    "vue.js",
    "material design"
  ],
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "@mdi/js": "3.2.89",
    "@vue/test-utils": "1.0.0-beta.24",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "23.6.0",
    "jest": "23.6.0",
    "jest-serializer-vue": "2.0.2",
    "mustache": "^3.0.1",
    "regenerator-runtime": "^0.13.1",
    "standard-version": "4.4.0",
    "vue": "^2.5.21",
    "vue-jest": "^3.0.2",
    "vue-template-compiler": "^2.5.21"
  },
  "homepage": "https://gitlab.com/robcresswell/vue-material-design-icons",
  "bugs": {
    "url": "https://gitlab.com/robcresswell/vue-material-design-icons/issues"
  },
  "scripts": {
    "prebuild": "npm install && npm run clean",
    "build": "./build.js && cp styles.css dist/",
    "clean": "rm -rf dist/",
    "pretest": "npm run build",
    "test": "jest",
    "prerelease": "npm run build",
    "release": "standard-version --sign --tag-prefix '' && cp package.json README.md CHANGELOG.md dist/ && cd dist && npm publish"
  }
}
